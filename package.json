{
  "name": "learner-model-gql",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/PabloSzx/learner-model-gql#readme",
  "bugs": {
    "url": "https://github.com/PabloSzx/learner-model-gql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PabloSzx/learner-model-gql.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "type": "module",
  "scripts": {
    "clean:install": "pnpm dlx rimraf \"**/{node_modules,.next,coverage,dist}\" pnpm-lock.yaml && pnpm i && pnpm generate",
    "clean:test:db": "docker-compose down && sudo rimraf .test-postgres-data",
    "predev": "pnpm -r db:generate",
    "dev": "concurrently -r \"cross-env NEXT_PUBLIC_API_URL=http://localhost:8080/graphql pnpm -r --no-bail dev\" \"tsc --watch\"",
    "generate": "pnpm -r generate",
    "postinstall": "node patch.cjs",
    "prepare": "husky install",
    "pretty": "pretty-quick",
    "pretty:all": "prettier --write \"**/*.{ts,tsx,gql,json}\"",
    "test": "c8 mocha",
    "test:watch": "bob-watch -w \"packages/**/*.ts\" test -c mocha --ignore \"**/generated/**\""
  },
  "dependencies": {
    "api-base": "workspace:^0.0.1",
    "common": "workspace:^0.0.1",
    "graph": "workspace:^0.0.1",
    "undici": "^4.8.2"
  },
  "devDependencies": {
    "@graphql-ez/fastify": "^0.9.4",
    "@graphql-tools/code-file-loader": "^7.2.1",
    "@graphql-tools/graphql-tag-pluck": "^7.1.2",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@types/concurrently": "^6.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "@types/prettier": "^2.4.1",
    "@types/randomstring": "^1.1.7",
    "@types/react": "^17.0.33",
    "bob-tsm": "^0.3.2",
    "bob-watch": "^0.1.0",
    "c8": "^7.10.0",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.9",
    "execa": "^5.1.1",
    "fastify": "^3.22.1",
    "graphql": "^15.7.0",
    "graphql-ez": "^0.13.2",
    "husky": "^7.0.4",
    "json5": "^2.2.0",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "randomstring": "^1.2.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "testing": "workspace:*",
    "typescript": "^4.4.4",
    "valtio": "^1.2.5",
    "wait-on": "^6.0.0"
  },
  "engines": {
    "node": ">=v16.13.0"
  },
  "pnpm": {
    "overrides": {
      "ws@>=5.0.0 <5.2.3 || >=6.0.0 <6.2.2 || >=7.0.0 <7.4.6": ">=5.2.3 <6.0.0 || >=6.2.2 <7.0.0 || >=7.4.6",
      "tar@<4.4.16 || >=5.0.0 <5.0.8 || >=6.0.0 <6.1.7": ">=4.4.16 <5.0.0 || >=5.0.8 <6.0.0 || >=6.1.7",
      "tar@<4.4.18 || >=5.0.0 <5.0.10 || >=6.0.0 <6.1.9": ">=4.4.18 <5.0.0 || >=5.0.10 <6.0.0 || >=6.1.9"
    }
  }
}
