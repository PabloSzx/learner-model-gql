  "scripts": {
    "clean:dist": "rimraf \"{clients,packages}/*/dist\" gateway/dist",
    "dev": "concurrently pnpm:dev:*",
    "dev:api-base": "pnpm dev -r --filter api-base",
    "dev:common": "pnpm dev -r --filter common",
    "dev:db": "pnpm dev -r --filter db",
    "dev:gateway": "pnpm dev -r --filter gateway",
    "dev:next": "cross-env NEXT_PUBLIC_API_URL=http://localhost:8080/graphql pnpm dev -r --filter client",
    "dev:react-gql": "pnpm dev -r --filter react-graphql",
    "dev:service-actions": "pnpm dev -r --filter service-actions",
    "dev:service-content": "pnpm dev -r --filter service-content",
    "dev:service-domain": "pnpm dev -r --filter service-domain",
    "dev:service-projects": "pnpm dev -r --filter service-projects",
    "dev:service-state": "pnpm dev -r --filter service-state",
    "dev:service-users": "pnpm dev -r --filter service-users",
    "pretty": "pretty-quick",
    "pretty:all": "prettier --write \"**/*.{ts,tsx,gql,json}\"",
    "start": "pm2 start ecosystem.config.js",
    "test": "jest",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch"
  },
  "devDependencies": {
    "@types/concurrently": "^6.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.8",
    "@types/prettier": "^2.3.2",
    "@types/react": "^17.0.19",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "es-jest": "^1.3.0",
    "esbuild": "^0.12.24",
    "graphql": "^15.5.2",
    "graphql-ez": "^0.12.6",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "json5": "^2.2.0",
    "pm2": "^5.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "valtio": "^1.2.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.12.20",
      "ws@>=5.0.0 <5.2.3 || >=6.0.0 <6.2.2 || >=7.0.0 <7.4.6": ">=5.2.3 <6.0.0 || >=6.2.2 <7.0.0 || >=7.4.6"
