{
  "name": "learner-model-gql",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/PabloSzx/learner-model-gql#readme",
  "bugs": {
    "url": "https://github.com/PabloSzx/learner-model-gql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PabloSzx/learner-model-gql.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "type": "module",
  "scripts": {
    "clean:install": "pnpm dlx rimraf \"**/{node_modules,rimraf,.next,coverage,dist}\" && pnpm i && pnpm generate",
    "clean:test:db": "docker-compose down && sudo rimraf .test-postgres-data",
    "dev:service": "pnpm prepack && pnpm generate && concurrently -r \"pnpm -r dev --no-sort --no-bail\" \"cross-env NEXT_PUBLIC_API_URL=http://localhost:8080/graphql pnpm -r dev:service --no-sort --no-bail\"",
    "generate": "pnpm -r generate --no-sort --no-bail",
    "prepare": "husky install && pnpm prepack",
    "prepack": "pnpm -r prepack --no-sort --no-bail",
    "pretty": "pretty-quick",
    "pretty:all": "prettier --write \"**/*.{ts,tsx,gql,json}\"",
    "start": "pm2 start ecosystem.config.js",
    "test": "bob-ts -i packages -i test && cross-env NODE_ENV=test c8 mocha",
    "test:watch": "concurrently -r pnpm:test:watch:cmd \"pnpm dev -r --no-sort --no-bail\"",
    "test:watch:cmd": "bob-ts-watch -i packages -i test -c \"cross-env NODE_ENV=test c8 mocha\"",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch"
  },
  "devDependencies": {
    "@graphql-ez/fastify": "^0.9.3",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@types/concurrently": "^6.2.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.13",
    "@types/prettier": "^2.3.2",
    "@types/randomstring": "^1.1.7",
    "@types/react": "^17.0.20",
    "api-base": "workspace:^0.0.1",
    "bob-ts": "^1.0.5",
    "c8": "^7.8.0",
    "common": "workspace:^0.0.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.25",
    "execa": "^5.1.1",
    "fastify": "^3.21.0",
    "graphql": "^15.5.3",
    "graphql-ez": "^0.12.7",
    "husky": "^7.0.2",
    "json5": "^2.2.0",
    "mocha": "^9.1.1",
    "pm2": "^5.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "randomstring": "^1.2.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "testing": "workspace:*",
    "typescript": "^4.4.2",
    "valtio": "^1.2.2",
    "wait-on": "^6.0.0"
  },
  "engines": {
    "node": ">=16.8"
  },
  "pnpm": {
    "overrides": {
      "ws@>=5.0.0 <5.2.3 || >=6.0.0 <6.2.2 || >=7.0.0 <7.4.6": ">=5.2.3 <6.0.0 || >=6.2.2 <7.0.0 || >=7.4.6",
      "tar@<4.4.16 || >=5.0.0 <5.0.8 || >=6.0.0 <6.1.7": ">=4.4.16 <5.0.0 || >=5.0.8 <6.0.0 || >=6.1.7",
      "tar@<4.4.18 || >=5.0.0 <5.0.10 || >=6.0.0 <6.1.9": ">=4.4.18 <5.0.0 || >=5.0.10 <6.0.0 || >=6.1.9"
    }
  }
}
