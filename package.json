    "pretty-quick": "^3.1.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^15.14.0",
    "dotenv": "^10.0.0",
    "pm2": "^5.1.0",
    "react": "^17.0.2",
    "valtio": "^1.0.6"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "valtio": "^1.0.5"
  },
  "pnpm": {
    "overrides": {
      "prisma": "2.27.0-integration-fix-sdk-pnpm.1",
