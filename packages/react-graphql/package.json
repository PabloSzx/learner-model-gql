{
  "name": "react-graphql",
  "version": "0.0.1",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "concurrently -r \"graphql-codegen --config codegen.yml --watch\" \"tsup --watch\"",
    "gql": "graphql-codegen --config codegen.yml",
    "prepare": "concurrently --raw -m 1 pnpm:gql pnpm:build"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/core": "^1.17.10",
    "@graphql-codegen/typed-document-node": "^1.18.9",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-document-nodes": "1.17.15",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-codegen/typescript-react-query": "^1.3.4",
    "@types/node": "^15.14.0",
    "common": "workspace:^0.0.1",
    "concurrently": "^6.2.0",
    "graphql": "^15.5.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-query": "^3.18.1",
    "tsup": "^4.12.0",
    "typescript": "^4.3.5",
    "valtio": "^1.0.6"
  },
  "peerDependencies": {
    "common": "workspace:^0.0.1",
    "graphql": "^15.5.0",
    "react": "^17.0.2",
    "react-query": "^3.16.0",
    "valtio": "^1.0.5"
  }
}
