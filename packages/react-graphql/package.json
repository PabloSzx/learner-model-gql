{
  "name": "react-graphql",
  "version": "0.0.1",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm prepare",
    "dev": "concurrently -r \"graphql-codegen --config codegen.yml --watch\" \"tsup --watch\"",
    "gql": "graphql-codegen --config codegen.yml",
    "prepare": "pnpm run gql && tsup"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/core": "^2.1.0",
    "@graphql-codegen/typed-document-node": "^2.1.2",
    "@graphql-codegen/typescript": "^2.1.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.2",
    "@graphql-codegen/typescript-operations": "^2.1.2",
    "@graphql-codegen/typescript-react-query": "^2.1.2",
    "@types/node": "^16.7.8",
    "common": "workspace:^0.0.1",
    "concurrently": "^6.2.1",
    "graphql": "^15.5.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-query": "^3.21.1",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2",
    "valtio": "^1.2.2"
  },
  "peerDependencies": {
    "common": "*",
    "graphql": "*",
    "react": "*",
    "react-query": "*",
    "valtio": "*"
  },
  "tsup": {
    "target": "node14",
    "entryPoints": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "silent": true
  }
}
