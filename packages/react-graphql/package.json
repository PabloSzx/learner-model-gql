{
  "name": "react-graphql",
  "version": "0.0.1",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm prepack",
    "dev": "concurrently -r \"graphql-codegen --config codegen.yml --watch\" \"bob-ts-watch -f interop\"",
    "generate": "graphql-codegen --config codegen.yml && bob-ts -f interop",
    "gql": "graphql-codegen --config codegen.yml",
    "prepack": "bob-ts -f interop"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/core": "^2.1.0",
    "@graphql-codegen/typed-document-node": "^2.1.4",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.4",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-query": "^3.0.1",
    "common": "workspace:^0.0.1",
    "concurrently": "^6.2.1",
    "graphql": "^15.6.0",
    "prettier": "^2.4.1",
    "react-query": "^3.24.3"
  }
}
