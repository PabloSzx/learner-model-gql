{
  "name": "graph",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./dist/*.js",
    "./gqty": "./dist/gqty/index.js",
    "./rq": "./dist/rq/index.js"
  },
  "scripts": {
    "build": "pnpm prepack",
    "dev": "concurrently -r pnpm:dev:*",
    "dev:rq": "graphql-codegen --config codegen.yml --watch",
    "dev:ts": "bob-ts-watch",
    "generate": "concurrently pnpm:generate:* && bob-ts",
    "generate:gqty": "gqty generate",
    "generate:rq": "graphql-codegen --config codegen.yml",
    "prepack": "bob-ts"
  },
  "dependencies": {
    "@gqty/logger": "^2.0.0",
    "@gqty/react": "^2.0.0",
    "common": "workspace:^0.0.1",
    "gqty": "^2.0.1",
    "graphql": "^15.6.0",
    "prettier": "^2.4.1",
    "react-query": "^3.24.4",
    "valtio": "^1.2.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/core": "^2.1.0",
    "@graphql-codegen/typed-document-node": "^2.1.4",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.4",
    "@graphql-codegen/typescript-operations": "^2.1.5",
    "@graphql-codegen/typescript-react-query": "^3.0.1",
    "bob-ts": "^1.1.1"
  }
}
