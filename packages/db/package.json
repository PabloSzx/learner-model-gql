{
  "name": "db",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "prisma db push",
    "dev": "concurrently -r \"tsup --watch src\" \"tsc --watch\"",
