{
  "name": "db",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "prisma db push",
    "dev": "tsup-node --watch src",
    "migrate:dev": "prisma migrate dev",
    "migrate:push": "prisma db push",
    "prepare": "prisma generate && tsup"
  },
  "dependencies": {
    "@prisma/client": "2.30.2",
    "common-api": "workspace:^1.0.0",
    "prisma": "2.30.2",
    "prisma-dbml-generator": "^0.7.0",
    "tsup": "^4.14.0"
  },
  "devDependencies": {
    "@types/concurrently": "^6.2.1",
    "@types/react": "^17.0.19"
  },
  "tsup": {
    "target": "node14",
    "entryPoints": [
      "src/index.ts"
    ],
    "silent": true
  }
}
