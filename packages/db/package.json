    "cross-env": "^7.0.3",
    "prisma": "2.23.0",
    "tsup": "^4.12.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "react": "^17.0.2",
    "valtio": "^1.0.6"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "valtio": "^1.0.5"
  }
}
